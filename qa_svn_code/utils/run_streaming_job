#!/usr/local/bin/perl

# The purpose of this script is to run a streaming job
# Example:
# $ CLUSTER=<cluster name> run_streaming_job
# $ CLUSTER=<cluster name> run_streaming_job -user hadoopqa
# $ CLUSTER=<cluster name> run_streaming_job -user hadoopqa -input /homes/hadoopqa/test_data/file100MB
# $ CLUSTER=<cluster name> run_streaming_job -user hadoopqa -input_type randomtextwriter
# $ CLUSTER=<cluster name> run_streaming_job -user hadoopqa -files ~/mapper.sh -mapper mapper.sh
# $ CLUSTER=<cluster name> run_streaming_job -queue grideng -user hadoopqa
# $ CLUSTER=<cluster name> run_streaming_job -args -D mapred.child.java.opts=-Xmx1024m -Dmapreduce.map.speculative=true
# $ CLUSTER=<cluster name> run_streaming_job -args -Dmapreduce.map.speculative=true

use strict;
use warnings;

use FindBin qw($Bin $Script);
use lib "$FindBin::Bin/../lib";
use base qw(Hadoop::Test);
use Getopt::Long;
use Test::More;

my %options = ();
my $verbose = 0;
my $input_type = 'local';
my $input;
my $files;
my $mapper = 'cat';
my $reducer = 'NONE';
my $user=getpwuid($>);
my @args;
my $result =
    GetOptions ( \%options,
                 "input=s"      => \$input,
                 "input_type=s" => \$input_type,
                 "files=s"      => \$files,
                 "mapper=s"     => \$mapper,
                 "reducer=s"    => \$reducer,
                 "user=s"       => \$user,
                 "verbose"      => \$verbose,
                 "args=s{,}"    => \@args,
                 "help|h|?");

usage(1) if (!$result) or (@ARGV);
usage(0) if $options{help};

sub usage
{
    my($exit_code) = @_;
    diag << "EOF";
Usage: $Script
   [-input       <input text file>   ]: Input text file. Default input will be auto-generated by randomtextwriter
   [-input_type  <input type>        ]: {'local', 'hdfs', 'randomtextwriter'} Default is local
   [-files       <file>              ]: distributed cache files
   [-args        <runtime args>      ]: one or more runtime args to be passed through
   [-user        <user uid>          ]: default user is the uid
   [-verbose                         ]: verbosity
   [-help|h|?                        ]: Display usage

Example:
CLUSTER=<cluster name> run_streaming_job
CLUSTER=<cluster name> run_streaming_job -user hadoopqa
CLUSTER=<cluster name> run_streaming_job -user hadoopqa -input /homes/hadoopqa/test_data/file100MB
CLUSTER=<cluster name> run_streaming_job -user hadoopqa -input_type randomtextwriter
CLUSTER=<cluster name> run_streaming_job -user hadoopqa -files ~/mapper.sh -mapper mapper.sh
CLUSTER=<cluster name> run_streaming_job -queue grideng -user hadoopqa
CLUSTER=<cluster name> run_streaming_job -args -D mapred.child.java.opts=-Xmx1024m -Dmapreduce.map.speculative=true
CLUSTER=<cluster name> run_streaming_job -args -Dmapreduce.map.speculative=true

EOF
    exit $exit_code;
}

my $self = __PACKAGE__->new;
my $Config = $self->config;

note("--> Run a streaming job in the foreground: ");

# Generate text input
if ($input_type eq 'randomtextwriter') {
    my $rtw_output = $self->run_randomtextwriter_job(undef, $verbose);
    $input = $rtw_output;
    $input_type = 'hdfs';
}
if ($input) {
    if  ($input_type eq 'local') {
        # copy it to hdfs
        my $input_copy = 'output_dir_' . time;
        $self->run_fs(['-copyFromLocal', $input, $input_copy], $user);
        $input = $input_copy;
    }
    # else it's a HDFS filename
}
else {
    # copy default input file to hdfs
    $input = '/etc/passwd';
    my $input_copy = 'output_dir_' . time;
    $self->run_fs(['-copyFromLocal', $input, $input_copy], $user);
    $input = $input_copy;
}
$self->run_fs(['-ls', $input], $user);

# streaming job
my $job_hash = { 'input_dir' => $input };
$job_hash->{mapper} = $mapper;
$job_hash->{reducer} = $reducer;
$job_hash->{files} = $files if $files;
$job_hash->{user} = $user if $user;
$job_hash->{args} = \@args if (@args);
my $output_dir = $self->run_streaming_job($job_hash, $verbose);
$self->run_fs(['-ls', $output_dir], $user);

# /home/gs/gridre/yroot.theoden/share/hadoop/bin/hadoop fs -copyFromLocal ~hadoopqa/test_data/file100MB file

