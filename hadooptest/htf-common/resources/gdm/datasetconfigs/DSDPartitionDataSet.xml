	</WorkflowStrategy>
	<Sources>
		<Source name='FDI_SERVER_NAME' latency='720' />
	</Sources>
	<DataFlow />
	<Targets>
		<Target latency="1440" name='TARGET1' status="active">
		 <DateRange>
         	<StartDate type="fixed" value="20120125"/>
