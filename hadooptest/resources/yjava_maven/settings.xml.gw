 <profiles>
   <profile>
     <id>localMirror</id>
     <activation>
       <activeByDefault>false</activeByDefault>
     </activation>
     <pluginRepositories>
       <pluginRepository>
         <id>local-mirror</id>
         <name>local mirror of central repo</name>
         <layout>default</layout>
         <url>file:///home/y/var/maven</url>
         <releases>
           <enabled>true</enabled>
         </releases>
         <snapshots>
           <enabled>false</enabled>
         </snapshots>
       </pluginRepository>
     </pluginRepositories>
     <repositories>
       <repository>
         <id>local-mirror</id>
         <name>local mirror of central repo</name>
