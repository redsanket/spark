<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<parent>
		<groupId>yahoo.yinst.yjava_base_build</groupId>
		<artifactId>yjava_base_build</artifactId>
		<version>3.1.6.198</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<groupId>yahoo.yinst.yjava_servlet_filters</groupId>
	<artifactId>yjava_servlet_filters</artifactId>
	<packaging>jar</packaging>
	<version>0.26.${build}</version>
	<name>yjava_servlet_filters</name>
	<url>http://twiki.corp.yahoo.com/view/JavaPlatform/PackageYJavaServletFilters</url>
	<description>Java Platform servlet filters</description>


	<properties>
		<bug.component>yjava_servlet_filters%20%26%20yjava_tomcat_valves</bug.component>
		<scm.path>yahoo/yjava/app_server/servlet_filters/trunk</scm.path>
		<mailingList.name>java-platform-devel</mailingList.name>
	</properties>

	<dependencies>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>servlet-api</artifactId>
			<version>2.5</version>
			<scope>provided</scope>
		</dependency>

		<dependency>
			<groupId>yahoo.yinst.bouncer_auth_java</groupId>
			<artifactId>bouncer_auth_java</artifactId>
			<version>0.12.105</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_byauth</groupId>
			<artifactId>yjava_byauth</artifactId>
			<version>0.8.596</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_filter_logic</groupId>
			<artifactId>yjava_filter_logic</artifactId>
			<version>0.29.242</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.14.2</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_servlet</groupId>
			<artifactId>yjava_servlet</artifactId>
			<version>0.9.51</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_yca</groupId>
			<artifactId>yjava_yca</artifactId>
			<version>0.17.82</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_yck</groupId>
			<artifactId>yjava_yck</artifactId>
			<version>0.15.134</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_yhdrs</groupId>
			<artifactId>yjava_yhdrs</artifactId>
			<version>0.10.175</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_ynet</groupId>
			<artifactId>yjava_ynet</artifactId>
			<version>0.7.47</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_ysecure</groupId>
			<artifactId>yjava_ysecure</artifactId>
			<version>1.15.85</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_ycore</groupId>
			<artifactId>yjava_ycore</artifactId>
			<version>0.9.125</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.yjava_ysecure_native</groupId>
			<artifactId>yjava_ysecure_native</artifactId>
			<version>1.15.173</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-mock</artifactId>
			<version>2.0.8</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-core</artifactId>
			<version>2.5.6</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.testng</groupId>
			<artifactId>testng</artifactId>
			<version>6.3.1</version>
			<scope>test</scope>
			<exclusions>
				<exclusion>
					<groupId>junit</groupId>
					<artifactId>junit</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>org.mockito</groupId>
			<artifactId>mockito-all</artifactId>
			<version>1.7</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>yahoo.yinst.jports_org_json__json</groupId>
			<artifactId>org_json__json</artifactId>
			<version>1.20090211_1</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>yahoo.yinst.maven_plugins</groupId>
				<artifactId>yjava_maven_plugins_yinst</artifactId>
				<version>0.4.15</version>
				<configuration>
					<yicfPrepend>print &lt;&lt; 'EOF';</yicfPrepend>
					<yicfAppend>EOF</yicfAppend>
					<sourceDirectory>target/yicf</sourceDirectory>
				</configuration>
				<executions>
					<execution>
						<id>generate-yicf</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>yicf</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<reporting>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>findbugs-maven-plugin</artifactId>
				<version>2.5</version>
				<configuration>
					<excludeFilterFile>findbugs_exclude.xml</excludeFilterFile>
				</configuration>
			</plugin>
		</plugins>
	</reporting>

</project>
